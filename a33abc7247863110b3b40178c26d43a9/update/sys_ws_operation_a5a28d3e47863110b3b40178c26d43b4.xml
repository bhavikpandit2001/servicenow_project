	var gr = new GlideRecord('x_1139630_projects_project');
    gr.query();
    
    var projects = [];
    
    while (gr.next()) {
        var project = {
            sys_id: gr.sys_id.toString(),
            number: gr.number.toString(),
            description: gr.description.toString(),
			name: gr.name.toString(),
			assigned_to: gr.assigned_to.toString(),
			expires_in: gr.expires_in.toString()
            // Add more fields as needed
        };
        
        projects.push(project);
    }
    
    // Send the list of incidents as the API response
    response.setBody({status: 200, message: "projects get successfully...",projects});
    response.setStatus(200); //

})(request, response);]]></operation_script>
